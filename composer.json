{
    "name": "campaignchain/campaignchain-ce",
    "license": "MIT",
    "type": "project",
    "description": "The CampaignChain Community Edition",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "packagist/yuicompressor-bin": "2.4.8",
        "packagist/cssembed-bin": "0.4.5",
        "campaignchain/distribution-ce": "dev-1.0.0-beta.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "campaignchain/operation-twitter": "1.0.0-alpha.4",
        "campaignchain/operation-facebook": "dev-1.0.0-beta.1",
        "campaignchain/operation-linkedin": "1.0.0-alpha.4",
        "campaignchain/location-website": "1.0.0-alpha.4",
        "campaignchain/dev-tools": "1.0.0-alpha.1"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "CampaignChain\\CoreBundle\\Composer\\ScriptHandler::initApp",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "CampaignChain\\CoreBundle\\Composer\\ScriptHandler::initApp",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
